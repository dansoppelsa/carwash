{
    "name": "dansoppelsa/carwash",
    "description": "Data scrubber for Laravel applications",
    "type": "library",
    "php": ">=8.0",
    "require": {
        "fakerphp/faker": "^1.21.0",
        "illuminate/database": "^8.40|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.40|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench" : "^6.0",
        "orchestra/database": "^6.0"
    },
    "autoload": {
        "psr-4" : {
            "Carwash\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Carwash\\" : "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Carwash\\CarwashProvider"
            ]
        }
    }
}
